local function JRGYS_fake_script() -- OpenBtn_14.LocalScript 
	local script = Instance.new('LocalScript', OpenBtn_14)

	-- LocalScript inside the TextButton
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local Workspace = game:GetService("Workspace")
	
	-- Function to safely find and destroy an object
	local function DestroyObject(Parent, ObjectName)
		local Object = Parent and Parent:FindFirstChild(ObjectName)
		if Object then
			Object:Destroy()
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		-- 1. Attempt to delete objects in ReplicatedStorage (Votekick/KickedBind events)
		local EventsFolder = ReplicatedStorage:FindFirstChild("Events")
		DestroyObject(EventsFolder, "Votekick")
		DestroyObject(EventsFolder, "KickedBind")
	
		-- 2. Attempt to delete objects in Workspace (Map assets)
		local MapFolder = Workspace:FindFirstChild("Map")
		DestroyObject(MapFolder, "OriginOffice")
	end)
end
coroutine.wrap(JRGYS_fake_script)()
local function ODIU_fake_script() -- OpenBtn_15.LocalScript 
	local script = Instance.new('LocalScript', OpenBtn_15)

	-- This is a LocalScript. Place it inside the TextButton you want to use for flight.
	
	local Player = game.Players.LocalPlayer
	local Button = script.Parent
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	local ContextActionService = game:GetService("ContextActionService")
	
	-- Configuration
	local FLY_SPEED = 60 -- Adjust this value to change how fast the player flies
	local MAX_FORCE_MULTIPLIER = 200000 -- A high number is needed to overcome gravity/inertia
	
	-- State Variables
	local isFlying = false
	local HRP = nil -- HumanoidRootPart reference
	local BodyVelocity = nil -- Physics object used to control movement
	local HeartbeatConnection = nil -- Connection for the continuous update loop
	
	-- ----------------------------------------------------
	-- Helper Functions
	-- ----------------------------------------------------
	
	local function startFlight()
		local Character = Player.Character
		if not Character then return end
	
		HRP = Character:FindFirstChild("HumanoidRootPart")
		local Humanoid = Character:FindFirstChildOfClass("Humanoid")
		if not HRP or not Humanoid then return end
	
		-- 1. Set the flying state
		isFlying = true
		-- PlatformStand makes the character ignore gravity and standard platform collisions
		Humanoid.PlatformStand = true
	
		-- 2. Create and set up BodyVelocity to control physics
		BodyVelocity = Instance.new("BodyVelocity")
		BodyVelocity.MaxForce = Vector3.new(1, 1, 1) * MAX_FORCE_MULTIPLIER
		BodyVelocity.Velocity = Vector3.new(0, 0, 0) -- Initial velocity is zero (floats in place)
		BodyVelocity.Parent = HRP
	
		-- 3. Start the continuous update loop for movement
		HeartbeatConnection = RunService.Heartbeat:Connect(function()
			if not isFlying or not HRP or not BodyVelocity then
				-- Disconnect if something went wrong or flight was stopped externally
				if HeartbeatConnection then HeartbeatConnection:Disconnect() end
				return
			end
	
			local camera = workspace.CurrentCamera
			if not camera then return end
	
			local moveVector = Vector3.new(0, 0, 0)
	
			-- Get the standard WASD movement input from the player
			local moveDirection = Player:GetMoveDirection()
	
			-- Determine movement relative to the camera's CFrame
			local flatLookVector = camera.CFrame.lookVector * Vector3.new(1, 0, 1)
			local rightVector = camera.CFrame.rightVector
	
			-- Up/Down keys: Space (Up) and LeftControl or LeftShift (Down)
			local isUp = UserInputService:IsKeyDown(Enum.KeyCode.Space)
			local isDown = UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) or UserInputService:IsKeyDown(Enum.KeyCode.LeftShift)
	
			-- Calculate desired velocity based on inputs
			if moveDirection.Z > 0 then -- Forward
				moveVector = moveVector + flatLookVector
			elseif moveDirection.Z < 0 then -- Backward
				moveVector = moveVector - flatLookVector
			end
	
			if moveDirection.X > 0 then -- Right
				moveVector = moveVector + rightVector
			elseif moveDirection.X < 0 then -- Left
				moveVector = moveVector - rightVector
			end
	
			if isUp then
				moveVector = moveVector + Vector3.new(0, 1, 0) -- Move Up
			end
			if isDown then
				moveVector = moveVector - Vector3.new(0, 1, 0) -- Move Down
			end
	
			-- Apply speed
			if moveVector.Magnitude > 0 then
				BodyVelocity.Velocity = moveVector.unit * FLY_SPEED
			else
				BodyVelocity.Velocity = Vector3.new(0, 0, 0) -- Stop movement, stay floating in place
			end
		end)
	end
	
	local function stopFlight()
		isFlying = false
	
		-- Disconnect the update loop
		if HeartbeatConnection then
			HeartbeatConnection:Disconnect()
			HeartbeatConnection = nil
		end
	
		-- Clean up physics object
		if BodyVelocity then
			BodyVelocity:Destroy()
			BodyVelocity = nil
		end
	
		-- Restore normal character physics
		local Character = Player.Character
		if Character then
			local Humanoid = Character:FindFirstChildOfClass("Humanoid")
			if Humanoid then
				Humanoid.PlatformStand = false -- Allows player to fall/walk again
			end
		end
	end
	
	local function toggleFlight()
		if isFlying then
			stopFlight()
			Button.Text = "Activate Flight"
			Button.BackgroundColor3 = Color3.fromRGB(85, 170, 255) -- Blue
		else
			startFlight()
			Button.Text = "Deactivate Flight"
			Button.BackgroundColor3 = Color3.fromRGB(255, 85, 85) -- Red
		end
	end
	
	-- ----------------------------------------------------
	-- Initialization
	-- ----------------------------------------------------
	
	-- Wait for the character to load if it hasn't yet
	if not Player.Character then
		Player.CharacterAdded:Wait()
	end
	
	-- Connect the button click to the toggle function
	Button.MouseButton1Click:Connect(toggleFlight)
	
	-- Initial Button appearance setup (optional but recommended)
	Button.Text = "Activate Flight"
	Button.BackgroundColor3 = Color3.fromRGB(85, 170, 255)
	Button.TextColor3 = Color3.fromRGB(255, 255, 255)
	Button.Font = Enum.Font.SourceSansBold
	Button.TextSize = 24
end
coroutine.wrap(ODIU_fake_script)()
local function FDEE_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- Lua: LocalScript (Place this inside your TextButton)
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- Define the minimum speed allowed to prevent the player from getting stuck
	local MIN_SPEED = 5
	
	-- Connect a function to run every time the button is clicked
	button.MouseButton1Click:Connect(function()
	
		-- Ensure the player's character is loaded
		local character = player.Character or player.CharacterAdded:Wait()
	
		-- Find the Humanoid object that controls movement properties
		local humanoid = character:FindFirstChildOfClass("Humanoid")
	
		if humanoid then
			-- Calculate the new speed (subtract 1)
			local newSpeed = humanoid.WalkSpeed - 1
	
			-- Check if the new speed is greater than or equal to the minimum speed
			if newSpeed >= MIN_SPEED then
				-- THIS LINE DECREASES THE SPEED BY 1
				humanoid.WalkSpeed = newSpeed
				print("Speed successfully decreased to: " .. humanoid.WalkSpeed)
			else
				print("Cannot decrease speed further. Minimum speed reached: " .. MIN_SPEED)
			end
		end
	end)
end
coroutine.wrap(FDEE_fake_script)()
local function YRQZ_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	-- Lua: LocalScript (Place this inside your TextButton)
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- Connect a function to run every time the button is clicked
	button.MouseButton1Click:Connect(function()
	
		-- Ensure the player's character is loaded
		local character = player.Character or player.CharacterAdded:Wait()
	
		-- Find the Humanoid object that controls movement properties
		local humanoid = character:FindFirstChildOfClass("Humanoid")
	
		if humanoid then
			-- THIS LINE INCREASES THE SPEED BY 1
			humanoid.WalkSpeed = humanoid.WalkSpeed + 1
	
			print("Speed successfully increased to: " .. humanoid.WalkSpeed)
		end
	end)
end
coroutine.wrap(YRQZ_fake_script)()
local function TRDIG_fake_script() -- counter.LocalScript 
	local script = Instance.new('LocalScript', counter)

	-- Lua: LocalScript (Place this script directly inside your TextLabel)
	
	local speedDisplayLabel = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- Function to update the TextLabel with the current speed value
	local function updateSpeedDisplay(humanoid)
		if speedDisplayLabel and humanoid then
			-- Display ONLY the speed, rounded to the nearest whole number for clean output
			speedDisplayLabel.Text = math.floor(humanoid.WalkSpeed + 0.5)
		else
			-- Fallback text to show 0 if the character hasn't loaded yet
			speedDisplayLabel.Text = "0"
		end
	end
	
	-- Set up a continuous loop to check and update the speed display in real-time
	-- This runs separately and constantly without stopping.
	coroutine.wrap(function()
		-- Wait for the first character to load outside the loop, preventing errors on startup
		if not player.Character then
			player.CharacterAdded:Wait()
		end
	
		while true do
			wait(0.1) -- Update 10 times per second
	
			-- Safely get the current Humanoid inside the loop. 
			-- This avoids getting stuck if the character dies mid-game.
			local character = player.Character
			local humanoid = character and character:FindFirstChildOfClass("Humanoid")
	
			if humanoid then
				updateSpeedDisplay(humanoid)
			else
				-- If character/humanoid is missing (e.g., player died or is loading), update the display
				updateSpeedDisplay(nil) 
			end
		end
	end)()
end
coroutine.wrap(TRDIG_fake_script)()
local function MFIPQQI_fake_script() -- UpdatesButton.LocalScript 
	local script = Instance.new('LocalScript', UpdatesButton)

	local button = script.Parent
	local hub = button.Parent
	
	-- Constants (Must be shared across all toggle scripts for consistent behavior)
	local OPEN_ZINDEX = 5
	local CLOSED_ZINDEX = 1
	local WHITE = Color3.fromRGB(255, 255, 255)
	
	-- Define targets, grouping related Frame/Button pairs
	-- Note: We must define all three pairs here for coordination
	local targetGroups = {
		Main = {Frame = hub:FindFirstChild("MainFrame"), Button = hub:FindFirstChild("MainButton")},
		Items = {Frame = hub:FindFirstChild("ItemsFrame"), Button = hub:FindFirstChild("ItemsButton")},
		Updates = {Frame = hub:FindFirstChild("UpdatesFrame"), Button = button}, -- This button controls this frame
	}
	
	-- Basic safety check
	if not targetGroups.Main.Frame or not targetGroups.Items.Frame or not targetGroups.Updates.Frame then
		warn("Toggle Script (Updates): One or more essential UI elements are missing.")
		return
	end
	
	local function onButtonClicked()
		local updatesIsCurrentlyOpen = targetGroups.Updates.Frame.Visible
		local nextStateIsClosed = updatesIsCurrentlyOpen -- If it's currently open, the next click closes it
	
		if nextStateIsClosed then
			-- Action: CLOSE UpdatesFrame. When closing, all panels must be closed.
			targetGroups.Updates.Frame.Visible = false
			targetGroups.Updates.Frame.ZIndex = CLOSED_ZINDEX
	
			-- Un-highlight all buttons since no panel is active
			for _, group in pairs(targetGroups) do
				group.Button.TextColor3 = WHITE
			end
	
		else
			-- Action: OPEN UpdatesFrame and close all others.
	
			-- 1. Iterate and coordinate all panels (Main, Items, Updates)
			for name, group in pairs(targetGroups) do
				local isTarget = (name == "Updates")
	
				-- Set visibility and ZIndex
				group.Frame.Visible = isTarget
				group.Frame.ZIndex = isTarget and OPEN_ZINDEX or CLOSED_ZINDEX
	
				-- Set button color (Highlight/un-highlight state)
				group.Button.TextColor3 = WHITE
			end
		end
	
	
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
