-- Scripts:

local function SBKX_fake_script() -- OpenBtn.LocalScript 
	local script = Instance.new('LocalScript', OpenBtn)

	-- LocalScript placed inside the TextButton
	local Players = game:GetService("Players")
	local Workspace = game:GetService("Workspace")
	local Button = script.Parent
	
	-- Define constants
	local ITEM_FOLDER_NAME = "Items"
	local TOOL_NAME = "Frog Potion"
	local Y_OFFSET = 3
	
	Button.MouseButton1Click:Connect(function()
		local Player = Players.LocalPlayer
	
		-- Get character, waiting if necessary, and HumanoidRootPart
		local Character = Player.Character or Player.CharacterAdded:Wait()
		local HRP = Character:FindFirstChild("HumanoidRootPart")
	
		-- Find the target tool and its Handle part using short-circuit evaluation
		local TargetTool = Workspace:FindFirstChild(ITEM_FOLDER_NAME)
			and Workspace[ITEM_FOLDER_NAME]:FindFirstChild(TOOL_NAME)
	
		local TeleportPart = TargetTool and TargetTool:FindFirstChild("Handle")
	
		-- Teleport if all required parts exist (HRP and TeleportPart)
		if HRP and TeleportPart and TeleportPart:IsA("BasePart") then
			HRP.CFrame = TeleportPart.CFrame * CFrame.new(0, Y_OFFSET, 0)
		else
			-- Optional: Log a single warning for debugging if something is missing
			warn("Teleport failed: Missing HRP, 'Items' folder, 'Apple' tool, or 'Handle'.")
		end
	end)
end
coroutine.wrap(SBKX_fake_script)()
